{
  "name": "genpw.com",
  "version": "1.0.17",
  "private": true,
  "description": "Website for genpw",
  "homepage": "https://genpw.com",
  "bugs": {
    "url": "https://github.com/genpw/genpw.com/issues"
  },
  "license": "MIT",
  "author": "Matthew Miller <matthew@mi11er.net> (https://mi11er.net)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/genpw/genpw.com.git"
  },
  "scripts": {
    "build": "babel-node lib/build.js --buildOnly",
    "clean": "rimraf dist firebase-debug.log npm-debug.log yarn-error.log",
    "precommit": "run-s lint-staged pretty-quick nsp",
    "deploy": "run-s lint clean 'build -- --production' deploy:firebase",
    "postdeploy": "sonarwhal https://genpw.com >sonarwhal.scan",
    "deploy:firebase": "firebase deploy",
    "lint": "eslint --fix .",
    "lint-staged": "lint-staged",
    "nsp": "nsp check",
    "semantic-release": "semantic-release",
    "start": "babel-node lib/build.js",
    "test": "run-s lint clean build",
    "pretty-quick": "pretty-quick --staged"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "genpw": "2.0.0",
    "picnic": "6.5.0",
    "umbrellajs": "2.10.1"
  },
  "devDependencies": {
    "@mi11er/eslint-config": "2.2.0",
    "@semantic-release/changelog": "1.0.1",
    "@semantic-release/exec": "2.0.1",
    "@semantic-release/git": "3.0.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.3",
    "babel-preset-env": "1.6.1",
    "browser-sync": "2.23.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.18.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-no-unsafe-innerhtml": "1.0.16",
    "eslint-plugin-optimize-regex": "1.1.5",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-scanjs-rules": "0.2.1",
    "eslint-plugin-security": "1.4.0",
    "firebase-tools": "3.17.4",
    "husky": "0.14.3",
    "jstransformer-pug": "0.3.0",
    "lint-staged": "7.0.0",
    "metalsmith": "2.3.0",
    "metalsmith-layouts": "2.1.0",
    "metalsmith-markdown": "0.2.2",
    "metalsmith-sass": "1.5.1",
    "minimist": "1.2.0",
    "modernizr": "3.5.0",
    "modernizr-loader": "1.0.1",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "nsp": "3.2.1",
    "prettier": "1.10.2",
    "pretty-quick": "1.4.1",
    "pug": "2.0.0-rc.4",
    "pug-cli": "1.0.0-alpha6",
    "queue": "4.4.2",
    "rimraf": "2.6.2",
    "semantic-release": "13.0.1",
    "sonarwhal": "0.27.0",
    "sort-package-json": "1.11.0",
    "webpack": "4.0.1",
    "webpack-merge": "4.1.2"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "publish": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/exec",
        "cmd": "tar -czvf Site_Build.tgz dist firebase.json"
      },
      {
        "path": "@semantic-release/git",
        "assets": ["package.json", "CHANGELOG.md"]
      },
      {
        "path": "@semantic-release/exec",
        "cmd": "npm run deploy:firebase"
      },
      {
        "path": "@semantic-release/github",
        "assets": [
          {
            "path": "Site_Build.tgz",
            "label": "Site Build"
          }
        ]
      }
    ]
  }
}
